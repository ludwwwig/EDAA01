package bst;

public class Test {

	public static void main(String[] args) {
		BinarySearchTree<Integer> a = new BinarySearchTree<Integer>();
		
		System.out.println(a.add(10);
		a.add(15);
		a.add(5);
		a.printTree();
		System.out.println(a.size());
		BSTVisualizer abc = new BSTVisualizer("tree", 300,300);
		abc.drawTree(a);
		a.rebuild();
	}

}
